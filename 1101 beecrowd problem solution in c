#include <stdio.h>
int main()
{
    int m,n,sum=0,i,j,temp;
    while(1){
        scanf("%d%d",&m,&n);
        if(m<=0 || n<=0)
            break;
        else if(m>n){
            temp=m;
            m=n;
            n=temp;
        }
        for(i=m;i<=n;i++)
       {
            printf("%d ",i);
            sum=sum+i;
       }
       printf("Sum=%d\n",sum);
       sum=0;
    }



    return 0;
}
